<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="ru.kpfu.itis.paramonov.heartstone.controller.RegisterController"
            prefHeight="720.0" prefWidth="1280.0">

    <VBox layoutX="580" layoutY="200" fx:id="registerMenu" stylesheets="text-fields.css" spacing="10">
        <TextField fx:id="login" promptText="login"/>
        <PasswordField fx:id="password" promptText="password"/>
        <PasswordField fx:id="confirmPassword" promptText="confirm password"/>
    </VBox>

</AnchorPane>
